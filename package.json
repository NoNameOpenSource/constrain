{
    "name": "@nonameopensource/constrain",
    "version": "0.0.0",
    "description": "Constraint-based layout for the web",
    "main": "dist/index.js",
    "types": "types/index.d.ts",
    "scripts": {
        "compile": "rm -rf dist && tsc",
        "watch": "tsc -w true",
        "prepack": "npm run compile",
        "lint": "eslint --ext ts src/*",
        "fix": "prettier --config .prettierrc.yml 'src/**/*.ts' --write"
    },
    "config": {
        "fund": false
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/NoNameOpenSource/constrain.git"
    },
    "author": "NoNameOpenSource Team",
    "bugs": {
        "url": "https://github.com/NoNameOpenSource/constrain/issues"
    },
    "homepage": "https://github.com/NoNameOpenSource/constrain#readme",
    "devDependencies": {
        "@nonameopensource/eslint-config-typescript": "^1.2.1",
        "@tsconfig/node16": "^1.0.2",
        "eslint": "^8.13.0",
        "prettier": "^2.6.2",
        "source-map-support": "^0.5.21",
        "typescript": "^4.6.3"
    },
    "dependencies": {
        "zod": "^3.17.10"
    }
}
